<!DOCTYPE html>
<html lang="en">
  <%- include('partials/header') %>
  <body>
    <h1 class="form-head">Join Meeting</h1>
    <div class="container">
      <div class="container">
        <form
          action="/join"
          method="POST"
          class="needs-validation form"
          novalidate
        >
          <div class="join-form-cont">
            <label id="validationCustom01" class="form-label"
              >Meeting id:
            </label>
            <input
              type="text"
              name="id"
              id="validationCustom01"
              class="join-input"
            />
            <button class="btn btn-success join-btn">Join</button>
          </div>
        </form>
      </div>
    </div>
    <%- include('partials/footer') %>
  </body>
</html>
